{
    "build": {
      "env": {
        "NEXT_PUBLIC_API_URL": "https://chattext-back.onrender.com"
      }
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  